<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="httpclient.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:httpclient.Views"
    Shell.FlyoutBehavior="Disabled"
    Title="httpclient">


    <TabBar>
        <ShellContent
           Title="Home"
           ContentTemplate="{DataTemplate local:PostView}"
           Route="MainPage" />
        <ShellContent
            Title="Criar postagens"
            ContentTemplate="{DataTemplate local:CreatePostView}"
            Route="CreatePostView"/>
    </TabBar>
    

</Shell>
